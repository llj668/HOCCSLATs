            <Insets right="200.0" />
         </padding>
      </Label>
      <JFXButton fx:id="btnBack" layoutX="26.0" layoutY="14.0" onAction="#onClickBack" prefHeight="63.0" prefWidth="117.0" ripplerFill="BLACK" text="Back">
